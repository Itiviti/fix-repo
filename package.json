{
  "name": "fix-repo",
  "version": "0.1.0",
  "description": "JS library for FIX repository information",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "src/",
    "resources/"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "FIX",
    "FIX repository",
    "typescript"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "got": "^9.0.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-jest": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "merge2": "^1.2.2",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
